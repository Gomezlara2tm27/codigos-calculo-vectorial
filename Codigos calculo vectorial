Código hola, 5+6
#include<iostream>
float x=0;

int main(){
    x=5+6;
    std::cout<<"hola polinesios"<<std::endl;
    std::cout<<x<<std::endl;
    return 0;
}

Código distancia entre 2 puntos
#include<iostream>
using namespace std;
int main(){
    float x1=5;
    float x2=-4;
    float y1=5;
    float y2=2;
    float P1P2=0;
    float z1=0;
    float z2=0;
    P1P2= sqrt(((x2-x1)*(x2-x1))+((y2-y1)*(y2-y1))+((z2-z1)*(z2-z1)));
    cout<<P1P2;
}

Código producto entre 2 vectores
#include<iostream>
#include<cmath>
using namespace std;
int main(){
    float U1,U2,U3,V1,V2,V3,W;
    U1=1;
    U2=2;
    U3=3;
    V1=4;
    V2=5;
    V3=6;
    W=(U1*V1)+(U2*V2)+(U3*V3);
    cout <<W<<endl;
}

Código de coordenadas
#include <iostream>
#include <cmath>
using namespace std; 

float x=0;
float y=7.07;
float z=7.07;
float r=0;
float teta=0;
float phy=0;
int main (){
    
r=sqrt((x*x)+(y*y)+(z*z));
teta=atan(y/x);
phy=acos(z/(sqrt((x*x)+(y*y)+(z*z))));
cout<<"("<<r<<","<<teta<<","<<phy<<")";
return 0;
}

Código coordenadas esféricas rectas
#include<iostream>
#include<cmath>
#define PI 3.14159

using namespace std;

int main(){
//definimos las variables
    double teta=0;
    double phy=0;
    double r=0;
    double x=0;
    double y=0;
    double z=0;
//ecuaciones
    x=r*sin(phy)*cos(teta);
    y=r*sin(phy)*cos(teta);
    z=r*sin(phy);
    
    cout<<"("<<x<<","<<y<<","<<z<<")";
    return 0;
}

Código 3D[1]
#include<iostream>
#include<cmath>
using namespace std; 
int main (){
    float x1=3;
    float x2=(-7);
    float y1=(-5);
    float y2=6;
    float z1=2;
    float z2=(-4);
    float p1p2=0;
    p1p2=sqrt(((x2-x1)*(x2-x1))+((y2-y1)*(y2-y1))+((z2-z1)*(z2-z1)));
    cout<<p1p2<<endl;
    return 0;
}

